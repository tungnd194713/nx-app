{
  "name": "node-nx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "dep-graph": "nx dep-graph",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "import-sort": "import-sort --write '{apps,libs}/**/*.ts'",
    "lint": "nx lint",
    "nx": "nx",
    "replace-placeholders-in-build": "node ./replace.build.js",
    "start": "node dist/packages/nx-api/main",
    "start:client": "nx run packages/frontend:serve --configuration=development-en --hmr -o",
    "start:server": "nx run packages/nx-api:serve --watch",
    "build:dev": "nx run packages/nx-api:build && nx run packages/frontend:build && yarn replace-placeholders-in-build",
    "build:production": "nx run nx-api:build:production && nx run packages/frontend:build:production && yarn replace-placeholders-in-build",
    "test": "npx dotenv-cli -e .env.example -- nx test",
    "ts-node": "ts-node",
    "update": "nx migrate latest",
    "watch:server": "nx run packages/nx-api:build --watch",
    "watch:test": "nx test --watch",
    "workspace-generator": "nx workspace-generator",
    "database:docker": "open -a docker && sleep 5; docker run --name mysql -p 3306:3306 -e MYSQL_ROOT_PASSWORD=password -e MYSQL_DATABASE=database  -e MYSQL_USER=username -e MYSQL_PASSWORD=password -d mysql:latest",
    "test:be": "nx run packages/nx-api:test"
  },
  "private": true,
